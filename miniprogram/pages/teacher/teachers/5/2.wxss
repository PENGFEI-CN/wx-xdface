page{
  background-color: rgb(234, 235, 232);
  height: 100%;
}
.tr{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.th,.td {
  padding: 5px;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width:100%;
}
.th {
  font-weight: 400;
  background-color: #eaeff0;
}
.table {
  background-color: white;
  margin:20rpx auto;
  border:1px solid white;
  border-right: 0;
  border-bottom: 0;
  width: 94%;
  box-shadow: 0rpx 2rpx 2rpx #dadada;
}

.user-motto-end{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size:40rpx;
  margin:2%;
  width: 50%;
  height: 30%;
  background-color:white; 

}
.usermotto-end {
  display: flex;
  flex-direction: row;
  justify-content: center;
  position: fixed;
  bottom: 1%;
  left:3%;
  width:94%;
  background-color: white;
  box-shadow: 0px 1px 4px #c2b9b9 ;
  border:1px solid #dadada;
}
image{
  width:100rpx; 
  height:100rpx;
}