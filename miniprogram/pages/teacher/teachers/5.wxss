.result{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 200rpx;
}
.test{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 90%;
  margin-top:10rpx;
  margin-bottom: 10rpx;
  padding: 10rpx;
  box-sizing: border-box;
  border-style: solid;
  border-width: 0rpx 0rpx medium 0rpx;
  border-color:  #dadfde;
}
.uploader {
  height: 100rpx;
  width: 95%;
  background: #fff;
  border: 1px solid rgba(0, 0, 0, 0.1);
  display: flex;
  flex-direction: row;
  align-items: center;
  padding:0 20rpx;
  box-sizing: border-box; 
  margin: 0 auto;
  box-shadow: 0px 1px 4px #c2b9b9 ;
}
.button{
  border: 1px solid rgba(0, 0, 0, 0.1);
  margin:100rpx auto;
  width:500rpx;
  background-color: #d4dcdd5e;
  box-shadow: 0px 1px 4px #c2b9b9 ;
  border-radius:100rpx;
}
.button2{
  border: 1px solid rgba(0, 0, 0, 0.1);
  margin:50rpx auto;
  width:500rpx;
  background-color: #d4dcdd5e;
  box-shadow: 0px 1px 4px #c2b9b9 ;
  border-radius: 100rpx;
}
.input{
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 40rpx;
}
.Input {
  background: white;
  display: flex;
  align-items: center;
  flex-direction:column;
  align-content:center;
  box-shadow:  0rpx 0rpx 2rpx  96C2F1 ;
  border-radius:10rpx;
}

.button3{
  border: 2px solid rgba(0, 0, 0, 0.1);
  margin:150rpx auto;
  width:300rpx;
  background-color: rgb(235, 228, 228);
  box-shadow: 0px 1px 4px #c2b9b9 ;
  border-radius: 98rpx;
}

